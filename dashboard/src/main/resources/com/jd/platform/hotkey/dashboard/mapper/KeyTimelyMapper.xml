  <select id="listKeyTimely"  resultMap="BaseResultMap"  parameterType="com.jd.platform.hotkey.dashboard.common.domain.SearchDto">
    select
    <include refid="Base_Column_List" />
    from hk_key_timely
    where 1=1
    <if test="appName !=null and appName !=''">
       and app_name = #{appName}
    </if >
    <if test="key !=null and key !=''">
      and key_name = #{key}
    </if >
